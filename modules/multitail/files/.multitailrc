global_default_nkb:5MB
check_mail:0
tab_stop:4


# php error log
colorscheme:php_error:PHP Errors
cs_re:blue:^\[..-...-.... ..:..:..\]
cs_re:green,,bold:\*MSG\*:
cs_re:white,red:PHP (Notice|Warning):
cs_re:white,red,blink:PHP Fatal error:
cs_re_s:red,,bold:PHP (Notice|Warning|Fatal error): (.*)\sin\s
cs_re_s:yellow:\sin\s(.*)\son\sline\s([0-9]*)

cs_re:magenta:SAPAccounting.*$

#color schema for gearman
colorscheme:gearman_worker:Gearman Worker
cs_re_s:yellow:\): \[([0-9]+)\]
cs_re:green:Worker process "[^"]+" started
cs_re:red:Worker process "[^"]+" with pid [0-9]+ exited.*$
cs_re:red,,bold:Worker process "[^"]+" with pid [0-9]+ stopped.*$
cs_re:,red:Worker process "[^"]+" with pid [0-9]+ terminated.*$
